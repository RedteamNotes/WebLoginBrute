# WebLoginBrute 环境变量配置
# 复制此文件为 .env 并根据需要修改

# 安全配置
WEBLOGINBRUTE_SECRET=your-super-secret-key-here-change-this-immediately
WEBLOGINBRUTE_ENCRYPTION_KEY=your-32-character-encryption-key

# 日志配置
WEBLOGINBRUTE_LOG_LEVEL=INFO
WEBLOGINBRUTE_LOG_FILE=webloginbrute.log
WEBLOGINBRUTE_AUDIT_LOG=audit.log

# 性能配置
WEBLOGINBRUTE_MAX_MEMORY_MB=1024
WEBLOGINBRUTE_MEMORY_WARNING_THRESHOLD=80
WEBLOGINBRUTE_MEMORY_CRITICAL_THRESHOLD=95

# 网络配置
WEBLOGINBRUTE_TIMEOUT=30
WEBLOGINBRUTE_MAX_RETRIES=3
WEBLOGINBRUTE_USER_AGENT=WebLoginBrute/0.27.2

# 安全级别
WEBLOGINBRUTE_SECURITY_LEVEL=standard
WEBLOGINBRUTE_ENABLE_HEALTH_CHECK=true
WEBLOGINBRUTE_ENABLE_NETWORK_VALIDATION=true

# 会话管理
WEBLOGINBRUTE_SESSION_ROTATION_INTERVAL=300
WEBLOGINBRUTE_SESSION_LIFETIME=1800
WEBLOGINBRUTE_MAX_SESSION_POOL_SIZE=50

# 代理配置（可选）
# WEBLOGINBRUTE_PROXY_HTTP=http://proxy:8080
# WEBLOGINBRUTE_PROXY_HTTPS=https://proxy:8080
# WEBLOGINBRUTE_PROXY_USERNAME=proxy_user
# WEBLOGINBRUTE_PROXY_PASSWORD=proxy_pass

# 开发模式（仅用于开发环境）
# WEBLOGINBRUTE_DEBUG=false
# WEBLOGINBRUTE_VERBOSE=false 